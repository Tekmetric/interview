{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "lint": {},
    "typecheck": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "start": {
      "cache": true,
      "persistent": true
    },
    "generate:types": {
      "cache": false,
      "persistent": false
    },
    "test": {
      "inputs": ["./**/*.{js,jsx,ts,tsx}"],
      "outputs": ["reports/**"]
    }
  }
}
