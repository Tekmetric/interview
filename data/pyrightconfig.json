{
  "exclude": ["**/__pycache__"],
  "venvPath": "data",
  "venv": ".venv",
  "pythonVersion": "3.13",
  "typeCheckingMode": "standard",
  "reportMissingTypeStubs": false,
  "useLibraryCodeForTypes": true,
  "enableTypeIgnoreComments": true,
  "reportGeneralTypeIssues": "warning",
  "strictListInference": true,
  "strictDictionaryInference": true
}