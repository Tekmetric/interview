  "scripts": {
    "start": "vite dev",
    "build": "vite build",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "format": "prettier --write ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
